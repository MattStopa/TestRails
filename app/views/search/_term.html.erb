<div>
  Foods
  <% Food.all.where(published: true).each do |food| %>
      <a href="/nutrient/<%= food.slug %>" >
        <div class="mt-1">
            <div class="bg-white p-2 border rounded hover:bg-blue-100 cursor-pointer flex">

            <img src="<%= food.image_url %>" class="w-12 h-12 rounded"/>

            <div class="mt-3 ml-5 text-lg"><%= food.name %> </div>
            </div>
        </div>
      </a>
  <% end %>
  Tags
  <div>
    <% Tag.all.each do |tag| %>
      <%= render 'components/tag', {tag: tag }%>
      
    <% end %>
  </div>
</div>